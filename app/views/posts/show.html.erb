<div class="col-md-12">
  <div class="post">
    <%= link_to("Edit", edit_post_path(@post), class: "btn btn-primary pull-right")%>
    <%= link_to("Delete", post_path(@post),class: "btn btn-default pull-right", method: :delete, data: { comfirm: "Are you sure? "}) %>
  </div>
  <h2><%= @post.title %></h2>
  <p><%= @post.description %></p>
  <p><%= @post.date %></p>
  <p><%= @post.location %></p>
</div>
